PROC main:
  LOCAL array%(10),i%
  RANDOMIZE MINUTE*SECOND
  PRINT "Before:",
  i%=1
  WHILE i%<>11
    array%(i%)=100+INT(RND*900)
    PRINT array%(i%),
    i%=i%+1
  ENDWH
  PRINT
  PRINT "After: ",
  i%=1
  WHILE i%<>11
    IF array%(i%)<>array%(i%)/2*2
      array%(i%)=0
      PRINT " ",
    ENDIF
    PRINT array%(i%),
    i%=i%+1
  ENDWH
  GET
ENDP
