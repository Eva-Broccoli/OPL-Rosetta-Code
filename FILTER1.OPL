PROC main:
  LOCAL array1%(10),array2%(10),i%
  RANDOMIZE MINUTE*SECOND
  i%=1
  WHILE i%<>11
    array1%(i%)=100+INT(RND*900)
    i%=i%+1
  ENDWH
  i%=1
  WHILE i%<>11
    IF array1%(i%)=array1%(i%)/2*2
      array2%(i%)=array1%(i%)
    ELSE
      array2%(i%)=0
    ENDIF
    i%=i%+1
  ENDWH
  PRINT "Array 1:   Array 2:"
  i%=1
  WHILE i%<>11
    PRINT array1%(i%),"      ",array2%(i%)
    i%=i%+1
  ENDWH
  GET
ENDP
